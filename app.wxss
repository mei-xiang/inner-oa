/* 顶部栏 */

.nav {
  width: 100%;
  overflow: hidden;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 8888;
}

page{
	font-size: 26rpx;
}

.nav-title {
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
  font-size: 14px;
  font-weight: 400;
  letter-spacing: 2px;
  color: #fff;
}

.nav .back {
  content: "";
  display: block;
  position: absolute;
  bottom: 0;
  left: 15px;
  color: #fff;
  font-size: 30px;
}

.bg-main {
  background-color: #00a2e9;
}

.bg-gray {
  background-color: #f7f7f7;
}

.overflow {
  overflow: auto;
}

.hidden {
  overflow: hidden;
}

/*隐藏滚动条*/

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.clear::after {
  content: "";
  display: block;
  clear: both;
}

/* 弹出输入框 */

.toast-box {
  width: 100%;
  height: 100%;
  opacity: 1;
  position: fixed;
  top: 0px;
  left: 0px;
  z-index: 9999;
}

.toastbg {
  opacity: 0.4;
  background-color: black;
  position: absolute;
  width: 100%;
  min-height: 100vh;
}

.showToast {
  position: absolute;
  opacity: 1;
  width: 70%;
  margin-left: 15%;
  margin-top: 40%;
}

.toast-title {
  padding-left: 5%;
  background-color: #2196f3;
  color: white;
  padding-top: 2vh;
  padding-bottom: 2vh;
  border-top-right-radius: 16rpx;
  border-top-left-radius: 16rpx;
  font-size: 30rpx;
}

.toast-main {
  padding-top: 2vh;
  padding-bottom: 2vh;
  background-color: white;
  text-align: center;
}

.toast-input {
  margin-left: 5%;
  margin-right: 5%;
  border: 1px solid #ddd;
  padding-left: 2vh;
  padding-right: 2vh;
  padding-top: 1vh;
  padding-bottom: 1vh;
}

.btnBody {
  position: absolute;
  opacity: 1;
  width: 70%;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  border-radius: 4px;
  overflow: hidden;
  background: #fff;
}

.btnBody .text {
	box-sizing: border-box;
  width: 100%;
  padding: 10px 16px;
  font-size: 34rpx;
  border-bottom: 1px solid #f2f2f2;
}

.btnBody .model {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 34rpx;
  border-bottom: 1px solid #f2f2f2;
}

.btnBody .model:last-child {
  border-bottom: none;
}

.toast-button {
  display: flex;
}

.toast-button .button1 {
  width: 50%;
}

.toast-button .button2 {
  width: 50%;
}

.toast-button .button1 button {
  width: 100%;
  background-color: white;
  color: red;
  border-radius: 0px;
  border-bottom-left-radius: 16rpx;
}

.toast-button .button2 button {
  width: 100%;
  background-color: white;
  color: black;
  border-radius: 0px;
  border-bottom-right-radius: 16rpx;
}

.toast-input textarea {
	width: 100%;
  height: 80px;
  text-align: left;
  font-size: 30rpx;
}
