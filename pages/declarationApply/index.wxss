page{
  background: #F0F4F7;
}
.content{
  margin-top: 66px;
  box-sizing: border-box;
}
.topnav{
  box-sizing: border-box;
  height: 200rpx;
  background: #FFF;
}
.topnav .model{
  box-sizing: border-box;
  float: left;
  width: 25%;
  height: 100%;
  position: relative;
  background: #FFF;
}
.topnav .model:nth-child(4n){
  border-right: none;
}
.topnav .model .con{
  width: max-content;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  text-align: center;
}
.topnav .model .con icon{
  font-size: 50rpx;
}
.topnav .model .con .name{
  color: #101010;
  margin-top: 20rpx;
}
.information{
  margin-top: 10px;
  box-sizing: border-box;
  background: #FFF;
}
.information .title{
  width: 100%;
  height: 80rpx;
  padding: 0 16px;
  line-height: 80rpx;
  border-bottom: 1px solid #F2F2F2;
  font-size: 12px;
  color: #101010;
}
.information .con{
  box-sizing: border-box;
  padding: 0 16px;
}
.information .con .row{
  box-sizing: border-box;
  position: relative;
  height: 160rpx;
  border-bottom: 1px solid #F2F2F2;
}
.information .con .row .logo{
  box-sizing: border-box;
  width: max-content;
  position: absolute;
  top: 50%;
  left: 0;
  transform: translateY(-50%);
}
.information .con .row .logo icon{
  font-size: 40rpx;
  color: #2b8ec8;
}
.information .con .row .state{
  box-sizing: border-box;
  width: max-content;
  position: absolute;
  top: 50%;
  right: 0;
  transform: translateY(-50%);
  color: #2b8ec8;
  font-size: 12px;
}
.information .con .row .text{
  padding: 40rpx 120rpx 0 70rpx;
}
.information .con .row .text .name{
  font-size: 12px;
  color: #101010;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.information .con .row .text .time{
  margin-top: 20rpx;
  font-size: 10px;
  color: #ABABAB;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}