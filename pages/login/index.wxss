.logo{
  margin:45rpx auto;
  margin-top:230rpx;
  width:185rpx;
  height:185rpx;
  box-sizing:border-box;
}
.logo image{
  width: 100%;
  height: 100%;
  display: block;
}
.title{
  text-align: center;
  color: #000;
  font-size: 36rpx;
}
.content{
  margin-top:45rpx;
  padding:0 70rpx;
  box-sizing:border-box;  
}
.content .row{
  box-sizing:border-box;
  border-bottom:2rpx solid #F2F2F2;
  position:relative;
  height:90rpx;
  margin-bottom:25rpx;  
}
.content .row .label{
  box-sizing: border-box;
  line-height: 88rpx;
  width: 30%;
  height: 88rpx;
  color: #000;
  font-size: 28rpx;
}
.content .row input{
  box-sizing: border-box;
  position: absolute;
  top: 0;
  left: 0;
  padding-left: 30%;
  width: 100%;
  line-height: 88rpx;
  height: 88rpx;
  color: #000;
  font-size: 24rpx;
}
.login{
  margin: 80rpx 64rpx;
  background: #2B8EC8;
  color: #FFF;
  text-align: center;
  box-sizing: border-box;
  height: 88rpx;
  border-radius: 8rpx;
  line-height: 88rpx;
  font-size: 32rpx;
  font-weight: 500;
  letter-spacing: 4rpx;
}
.cont {
  /* color: #000; */
  text-align: center;
}
.text {
  line-height:72rpx;
/* color:#000; */
  font-size:22rpx;

}
.passLogin {
  text-align: center;
  color: #1AAD19;
  line-height: 60rpx;
}