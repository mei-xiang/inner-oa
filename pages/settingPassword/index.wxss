page{
  background: #F0F4F7;
}
.mid{
  margin-top: 40rpx;
  box-sizing: border-box;
  width: 100%;
  background: #FFf;
  padding-left: 32rpx;
  border-bottom: 2rpx solid #F2F2F2;
  border-top: 2rpx solid #F2F2F2;
}
.mid .row{
  box-sizing: border-box;
  width: 100%;
  border-bottom: 2rpx solid #F2F2F2;
  height: 44px;
  position: relative;
  line-height: 88rpx;
  color: #000;
  font-size: 28rpx;
  font-weight: 500;
  position: relative;
}
input {
  position: absolute;
  top:20rpx;
  left:120rpx;
}
input.last{
  left: 146rpx;
}
.mid .row:last-child{
  border-bottom: none;
}
.mid .row:last-child input{
  left:146rpx;
}
.buttonBody
{
	position: fixed;
  left:25%;
  box-sizing:border-box;
  height:80rpx;
  width:50%;
  background:#FFF;
  z-index:10;
  top:40%;  
}
.buttonBody .submitBtn{
  box-sizing: border-box;
  width: 100%;
  height: 80rpx;
  border-radius: 10rpx;
  background-color: rgba(43, 142, 200, 1);
  color: #FFF;
  font-size: 28rpx;
  border: none;
  line-height:80rpx;
}