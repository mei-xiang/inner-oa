page{
  background: #FFF;
}
.content{
  margin-top: 66px;
  box-sizing: border-box;
  background: #FFF;
  padding: 0 16px;
}
.row{
  box-sizing: border-box;
  width: 100%;
  height: 44px;
  border-bottom: 1px solid #F2F2F2;
  position: relative
}
.row .label{
  box-sizing: border-box;
  width: 30%;
  line-height: 44px;
  font-size: 12px;
  color: #101010;
}
.row .label.mustFill::before{
  content: '*';
  display: inline-block;
  color: red;
}
.row .main{
  box-sizing: border-box;
  position: absolute;
  top: 0;
  left: 0;
  padding-left: 30%;
  width: 100%;
  height: 44px;
  line-height: 44px;
  font-size: 12px;
  text-align: right;
  color: #000;
}
.row .main .picker{
  width: 100%;
  height: 100%;
  line-height: 44px;
  box-sizing: border-box;
  font-size: 12px;
  color: #101010;
  position: relative;
}
.row .main input{
  width: 100%;
  height: 100%;
  line-height: 44px;
  box-sizing: border-box;
  font-size: 12px;
  color: #101010;
  position: relative;
}

.row .main .infoText{
  width: 100%;
  height: 100%;
  line-height: 44px;
  box-sizing: border-box;
  font-size: 12px;
  color: #101010;
  position: relative;
  margin: 0;
  text-align: left;
}
.submitBtn{
  box-sizing: border-box;
  margin: 50px 32px;
  height: 40px;
  border-radius: 2px;
  background-color: rgba(43, 142, 200, 1);
  color: #FFF;
  font-size: 14px;
}